// https://www.onlinegdb.com/online_c++_compiler

// TODO: add map, heaps etc.

#include <iostream>
#include <queue>
#include <vector>
#include <climits>
using std::vector;
using std::priority_queue;
using std::endl;

auto hcmp = [](const vector<int>& a, const vector<int> b) {
    return a[1] < b[1];
};

int main()
{
    std::cout<<"Hello World" << INT_MAX <<endl;
    
    
    priority_queue<vector<int>, vector<vector<int> >, decltype(hcmp) > pq{hcmp};
    pq.push({9,10});
    std::cout<<pq.top()[0] << "," << pq.top()[1] << endl;
    pq.push({7,15});
    std::cout<<pq.top()[0] << "," << pq.top()[1] << endl;
    pq.push({12,12});
    std::cout<<pq.top()[0] << "," << pq.top()[1] << endl;

    return 0;
};
